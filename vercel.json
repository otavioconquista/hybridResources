{
  "functions": {
    "app/index.py": {
      "includeFiles": "JSONs/**"
    }
  },
  "builds": [
    {
      "src": "app/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/match_vaga",
      "dest": "app/index.py"
    },
    {
      "src": "/match_vagas", 
      "dest": "app/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}